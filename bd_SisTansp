-- Database: bd_SisTansp

-- DROP DATABASE bd_SisTansp;

CREATE DATABASE bd_SisTansp
  WITH OWNER = postgres
       ENCODING = 'UTF8'
       TABLESPACE = pg_default
       LC_COLLATE = 'pt_BR.utf8'
       LC_CTYPE = 'pt_BR.utf8'
       CONNECTION LIMIT = -1;
       
CREATE TABLE Adm (
	AdmID int not null primary key,
	nome varchar(50) not null,
	email varchar(50) not null,
	senha varchar(35) not null
	)
CREATE TEBLE Aluno(
	nome varchar(70),
	 cpf varchar(20), 
	 matricula varchar(30),
	 nascimento varchar(10),
	 instituicao varchar(30), 
	 contatoEmail varchar(70), 
	 contatoTelefone varchar(10), 
	 curso varchar(30),
	 cidadeInstituicao varchar(60),
	 horarios varchar(20)
)
